
public class Main {

	public static void main(String[] args) {
		
		Calcolatrice c = new Calcolatrice(4, 2);
		
		
		double  som = c.somma(4, 2);
		System.out.println(som);
		double  d = c.divisione(4, 2);
		System.out.println(d);
		double  m = c.moltiplicazione(4, 2);
		System.out.println(m);
		double  sot = c.sottrazione(4, 2);
		System.out.println(sot);
	

	}

}
